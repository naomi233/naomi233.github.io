{
  "name": "vuepress-theme-hope-template",
  "version": "2.0.0",
  "private": true,
  "description": "A project of vuepress-theme-hope",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vuepress-vite build docs",
    "clean-dev": "vuepress-vite dev docs --clean-cache",
    "dev": "vuepress-vite dev docs",
    "postinstall": "npx simple-git-hooks",
    "lint-md": "lint-md --fix docs/**/*",
    "prettier": "prettier -w .",
    "update-package": "pnpm dlx vp-update"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,scss,md}": "prettier --write",
    "*.md": "lint-md --fix"
  },
  "devDependencies": {
    "@naomi233/lint-md-cli": "^2.1.6",
    "@vuepress/bundler-vite": "2.0.0-rc.26",
    "@vuepress/plugin-back-to-top": "2.0.0-rc.120",
    "@vuepress/plugin-feed": "2.0.0-rc.120",
    "@vuepress/plugin-markdown-hint": "2.0.0-rc.120",
    "@vuepress/plugin-markdown-tab": "2.0.0-rc.120",
    "@vuepress/plugin-redirect": "2.0.0-rc.120",
    "@vuepress/plugin-search": "2.0.0-rc.120",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "sass-embedded": "^1.93.3",
    "simple-git-hooks": "^2.13.1",
    "vue": "^3.5.25",
    "vuepress": "2.0.0-rc.26",
    "vuepress-theme-hope": "2.0.0-rc.99"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}