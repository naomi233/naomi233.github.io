{
  "name": "vuepress-theme-hope-template",
  "version": "2.0.0",
  "private": true,
  "description": "A project of vuepress-theme-hope",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vuepress-vite build docs",
    "clean-dev": "vuepress-vite dev docs --clean-cache",
    "dev": "vuepress-vite dev docs",
    "postinstall": "npx simple-git-hooks",
    "lint-md": "lint-md --fix docs/**/*",
    "prettier": "prettier -w .",
    "update-package": "pnpm dlx vp-update"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,scss,md}": "prettier --write",
    "*.md": "lint-md --fix"
  },
  "devDependencies": {
    "@naomi233/lint-md-cli": "^2.1.6",
    "@vuepress/bundler-vite": "2.0.0-rc.20",
    "@vuepress/plugin-back-to-top": "2.0.0-rc.92",
    "@vuepress/plugin-feed": "2.0.0-rc.92",
    "@vuepress/plugin-markdown-hint": "2.0.0-rc.92",
    "@vuepress/plugin-markdown-tab": "2.0.0-rc.92",
    "@vuepress/plugin-redirect": "2.0.0-rc.92",
    "@vuepress/plugin-slimsearch": "2.0.0-rc.92",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.86.3",
    "simple-git-hooks": "^2.12.1",
    "vue": "^3.5.13",
    "vuepress": "2.0.0-rc.20",
    "vuepress-theme-hope": "2.0.0-rc.79"
  }
}
