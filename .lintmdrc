{
  "rules": {
    "space-around-alphabet": 1,
    "space-around-number": 1,
    "no-empty-code-lang": 1,
    "no-empty-url": 1,
    "no-empty-list": 1,
    "no-empty-code": 1,
    "no-empty-inline-code": 1,
    "no-empty-blockquote": 1,
    "no-special-characters": 1,
    "use-standard-ellipsis": 1,
    "no-fullwidth-number": 1,
    "no-space-in-link": 1,
    "no-multiple-space-blockquote": 1,
    "correct-title-trailing-punctuation": 1,
    "no-space-in-inline-code": 1
  }
}
